package graphStreamVisualizer;

import org.graphstream.graph.Graph;
import org.graphstream.graph.implementations.*;

import burlap.domain.singleagent.graphdefined.GraphDefinedDomain;

public class GraphStreamVisualizer {
	GraphDefinedDomain d;

	public GraphStreamVisualizer(GraphDefinedDomain d) {
		this.d = d;



	}

	public void render() {	
		Graph graph = new SingleGraph("Tutorial 1");	
		graph.addNode("A" );
		graph.addNode("B" );
		graph.addNode("C" );
		graph.addEdge("AB", "A", "B");
		graph.addEdge("BC", "B", "C");
		graph.addEdge("CA", "C", "A");	
	}
	
	public static void (Stringp[] args) {
		GraphDefinedDomain dg = getNStateChain(n);
		GraphStreamVisualizer
	}

}
